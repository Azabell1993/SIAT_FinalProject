{"ast":null,"code":"import axios from 'axios';\nimport store from '@/store';\naxios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\nexport default {\n  data() {\n    return {\n      memID: '',\n      memPW: ''\n    };\n  },\n\n  computed: {\n    loginMemID() {\n      let loginUserID = store.state.loginUser.memID;\n      loginUserID = this.memID;\n      return loginUserID;\n    }\n\n  },\n  methods: {\n    sendPost() {\n      console.log(this.memID, this.loginMemID);\n      axios.post('http://192.168.0.81:9292/loginProc', {\n        memID: this.loginMemID,\n        memPW: this.memPW\n      }).then(function (res) {\n        if (res.data.memID === this.loginMemID) {\n          console.log(\"ID가 일치합니다.\");\n        } else {\n          console.log(\"ID가 틀렸습니다.\");\n        }\n\n        console.log(res.data);\n        console.log(res.data.mem.memID);\n        console.log(res.data.mem.memRole.roleCode);\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    onClickRedirect() {\n      window.open(\"http://localhost:8080/\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAeA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAGAD,KAAK,CAACE,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,KAAK,EAAE;IAFF,CAAP;EAID,CANY;;EAObC,QAAQ,EAAE;IACRC,UAAS,GAAK;MACZ,IAAIC,WAAU,GAAIT,KAAK,CAACU,KAAN,CAAYC,SAAZ,CAAsBN,KAAxC;MACAI,WAAU,GAAI,KAAKJ,KAAnB;MACA,OAAOI,WAAP;IACF;;EALQ,CAPG;EAcbG,OAAO,EAAE;IACPC,QAAO,GAAK;MACVC,OAAO,CAACC,GAAR,CAAY,KAAKV,KAAjB,EAAwB,KAAKG,UAA7B;MACAT,KAAK,CAACiB,IAAN,CAAW,oCAAX,EAAiD;QAC/CX,KAAK,EAAE,KAAKG,UADmC;QAE/CF,KAAK,EAAE,KAAKA;MAFmC,CAAjD,EAIGW,IAJH,CAIQ,UAAUC,GAAV,EAAe;QACnB,IAAGA,GAAG,CAACd,IAAJ,CAASC,KAAT,KAAmB,KAAKG,UAA3B,EAAsC;UACpCM,OAAO,CAACC,GAAR,CAAY,YAAZ;QACF,CAFA,MAGK;UACHD,OAAO,CAACC,GAAR,CAAY,YAAZ;QACF;;QACAD,OAAO,CAACC,GAAR,CAAYG,GAAG,CAACd,IAAhB;QACAU,OAAO,CAACC,GAAR,CAAYG,GAAG,CAACd,IAAJ,CAASe,GAAT,CAAad,KAAzB;QACAS,