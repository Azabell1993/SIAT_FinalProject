{"ast":null,"code":"import axios from 'axios';\nimport store from '@/store';\naxios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\nexport default {\n  computed: {\n    memID: {\n      get() {\n        return store.state.loginUser.memID;\n      },\n\n      set(value) {\n        store.commit('updateloginUserID', value);\n      }\n\n    },\n    memPW: {\n      get() {\n        return store.state.loginUser.memPW;\n      },\n\n      set(value) {\n        store.commit('updateloginUserPW', value);\n      }\n\n    }\n  },\n  methods: {\n    sendPost() {\n      console.log(this.memID);\n      axios.post('http://192.168.0.81:9292/loginProc', {\n        memID: this.memID,\n        memPW: this.memPW\n      }).then(function (res) {\n        // console.log(this.memID)\n        if (res.data.memID == memID) {\n          console.log(\"ID가 일치합니다.\");\n        } else {\n          console.log(\"ID가 틀렸습니다.\");\n        }\n\n        console.log(res.data); // console.log(res.data.mem.memID)\n        // console.log(res.data.mem.memRole.roleCode)    \n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    onClickRedirect() {\n      window.open(\"http://localhost:8080/\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAeA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAEAD,KAAK,CAACE,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,eAAe;EACbC,QAAQ,EAAE;IACRC,KAAK,EAAE;MACLC,GAAE,GAAK;QACL,OAAON,KAAK,CAACO,KAAN,CAAYC,SAAZ,CAAsBH,KAA7B;MACD,CAHI;;MAILI,GAAE,CAAGC,KAAH,EAAU;QACTV,KAAK,CAACW,MAAN,CAAa,mBAAb,EAAiCD,KAAjC;MACH;;IANK,CADC;IASRE,KAAK,EAAE;MACLN,GAAE,GAAK;QACL,OAAON,KAAK,CAACO,KAAN,CAAYC,SAAZ,CAAsBI,KAA7B;MACD,CAHI;;MAILH,GAAE,CAAGC,KAAH,EAAU;QACTV,KAAK,CAACW,MAAN,CAAa,mBAAb,EAAiCD,KAAjC;MACH;;IANK;EATC,CADG;EAmBbG,OAAO,EAAE;IACPC,QAAO,GAAK;MACVC,OAAO,CAACC,GAAR,CAAY,KAAKX,KAAjB;MACAN,KAAK,CAACkB,IAAN,CAAW,oCAAX,EAAiD;QAC/CZ,KAAK,EAAE,KAAKA,KADmC;QAE/CO,KAAK,EAAE,KAAKA;MAFmC,CAAjD,EAIGM,IAJH,CAIQ,UAAUC,GAAV,EAAe;QA