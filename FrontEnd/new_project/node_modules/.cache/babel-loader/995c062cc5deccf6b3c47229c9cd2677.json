d=\"check_2\" class=\"normal\" v-model=\"check_2\">\n        <label for=\"check_2\">서비스 이용약관 동의</label><br>\n        \n        <input type=\"checkbox\" id=\"check_3\" class=\"normal\" v-model=\"check_3\">\n        <label for=\"check_3\">마케팅 수신 동의</label><br>\n        <input type=\"reset\" value=\"초기화\">\n        <input type=\"submit\" id=\"check_submit\" value=\"회원가입 하러가기\">\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\naxios.defaults.headers.common['Access-Control-Allow-Origin'] = '*'\n\nexport default {\n  allClick() {\n            // 체크박스 전체 선택\n            $(\".checkbox_group\").on(\"click\", \"#check_all\", function () {\n            $(this).parents(\".checkbox_group\").find('input').prop(\"checked\", $(this).is(\":checked\"));\n            });\n        },\n        aloneClick() {\n            // 체크박스 개별 선택\n            $(\".checkbox_group\").on(\"click\", \".normal\", function() {\n                var is_checked = true;\n                $(\".checkbox_group .normal\").each(function(){\n                    is_checked = is_checked && $(this).is(\":checked\");\n                });\n\n                $(\"#check_all\").prop(\"checked\", is_checked);\n            });\n        },\n        joinSignupPage() {\n                        if(!(this.check_1)) {\n                alert(\"개인정보 처리방침 동의를 체크하십시오.\");\n                return false;\n            } else if(!(this.check_2)) {\n                alert(\"서비스 이용약관 동의를 체크하십시오.\");\n                return false;\n            } else if(!(this.check_3)) {\n                alert(\"마케팅 수신 동의를 체크하십시오.\");\n                return false;\n            } else {\n                axios.post(\"../views/SignView.vue\", signupMember)\n                .then((res) => {\n                let token = res.data.token\n                localStorage.setItem(\"access_token\", token)\n                this.$store.dispatch(\"getAccountInfo\")\n                this.$router.push({ name: \"Home\" })\n                }).catch((err) => {\n                if (err.response) {\n                    this.isError = true\n                    this.errorMsg = err.response.data.message\n                }\n                })\n            }\n        }\n} // export default\n</script>\n\n"]},"metadata":{},"sourceType":"module"}