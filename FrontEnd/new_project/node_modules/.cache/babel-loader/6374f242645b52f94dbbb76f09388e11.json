{"ast":null,"code":"import axios from 'axios';\nimport store from '@/store';\naxios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\nexport default {\n  computed: {\n    memID: {\n      get() {\n        return store.state.loginUser.memID; //store에 있는 값 가져오기 (초기값을 가져옴)\n      },\n\n      set(value) {\n        this.$store.commit('updateloginUserID', value); //input에 친 값을 다시 세팅\n      }\n\n    }\n  },\n  methods: {\n    sendPost() {\n      const userId = this.memID;\n      console.log(userId);\n      axios.post('http://192.168.0.107:9292/loginProc', {\n        memID: this.memID,\n        memPW: this.memPW\n      }).then(function (res) {\n        console.log(res);\n\n        if (res.data.memID == userId) {\n          console.log(\"ID가 일치합니다.\");\n        } else {\n          console.log(\"ID가 틀렸습니다.\");\n        } //store.commit('updateloginUserID',this.memID)\n\n\n        console.log(`login : ${store.state.loginUser.memID}`);\n      }).catch(function (error) {\n        console.log(error);\n      });\n      console.log(\"end\");\n    },\n\n    onClickRedirect() {\n      window.open(\"http://localhost:8080/\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAEAD,KAAK,CAACE,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,eAAe;EACbC,QAAQ,EAAE;IACRC,KAAK,EAAE;MACLC,GAAE,GAAK;QACL,OAAON,KAAK,CAACO,KAAN,CAAYC,SAAZ,CAAsBH,KAA7B,CADK,CAC8B;MACpC,CAHI;;MAILI,GAAG,CAACC,KAAD,EAAQ;QACT,KAAKC,MAAL,CAAYC,MAAZ,CAAmB,mBAAnB,EAAwCF,KAAxC,EADS,CACsC;MACjD;;IANK;EADC,CADG;EAWbG,OAAO,EAAE;IACPC,QAAO,GAAK;MACV,MAAMC,MAAK,GAAI,KAAKV,KAApB;MACAW,OAAO,CAACC,GAAR,CAAYF,MAAZ;MACAhB,KAAK,CAACmB,IAAN,CAAW,qCAAX,EAAkD;QAChDb,KAAK,EAAE,KAAKA,KADoC;QAEhDc,KAAK,EAAE,KAAKA;MAFoC,CAAlD,EAIGC,IAJH,CAIQ,UAAUC,GAAV,EAAe;QACnBL,OAAO,CAACC,GAAR,CAAYI,GAAZ;;QACA,IAAGA,GAAG,CAACC,IAAJ,CAASjB,KAAT,IAAkBU,MAArB,EAA4B;UAC1BC,OAAO,CAACC,GAAR,CAAY,YAAZ;QACF,CAFA,MAGK;UACHD,OAAO,CAACC,GAAR,CAAY,YAAZ;QACF,CAPmB,CAQnB;;;QACAD,OAAO,CAACC,GAAR,CAAa,WAAUjB,KAAK,CAACO,KAAN,CAAYC,SAAZ,CAAsBH,KAAM,EAAnD;MACD,CAdH,EAcKkB,KAdL,CAcW,UAAUC,KAAV,EAAiB;QACxBR,OAAO,CAACC,GAAR,CAAYO,KAAZ;MACD,CAhBH;MAiBER,OAAO,CAACC,GAAR,CAAY,KAAZ;IACH,CAtBM;;IAuBPQ,eAAc,GAAI;MACfC,MAAM,CAACC,IAAP,CAAY,wBAAZ;IACH;;EAzBO;AAXI,CAAf","names":["axios","store","defaults","headers","common","computed","memID","get","state","loginUser","set","value","$store","commit","methods","sendPost","userId","console","log","post","memPW","then","res","data","catch","error","onClickRedirect","window","open"],"sourceRoot":"","sources":["C:\\study\\project\\SIAT_FinalProject\\FrontEnd\\new_project\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\n    <div>\n    <h1>Login 페이지입니다.</h1>\n    \n    <form @submit.prevent=\"sendPost\">  <!-- sendPost 함수 실행 -->\n        <label for=\"memID\">ID : <input id=\"memID\" v-model=\"memID\" type=\"text\"></label><br><br>\n        <label for=\"memPW\">PW : <input id=\"memPW\" v-model=\"memPW\" type=\"password\"></label><br><br>\n        <button>Send</button><br>\n        <!-- @click=\"onClickRedirect()\" 로그인 성공시 화면 이동 -->\n        {{ memID }}\n    </form>\n    </div>\n</template>\n<script>\nimport axios from 'axios'\nimport store from '@/store'\n\naxios.defaults.headers.common['Access-Control-Allow-Origin'] = '*'\n\nexport default {\n  computed: {\n    memID: {\n      get () {\n        return store.state.loginUser.memID //store에 있는 값 가져오기 (초기값을 가져옴)\n      },\n      set(value) {\n        this.$store.commit('updateloginUserID', value) //input에 친 값을 다시 세팅\n      }\n    }\n  },\n  methods: {\n    sendPost () {\n      const userId = this.memID; \n      console.log(userId);\n      axios.post('http://192.168.0.107:9292/loginProc', {\n        memID: this.memID,\n        memPW: this.memPW\n      })\n        .then(function (res) {\n          console.log(res)\n          if(res.data.memID == userId){\n            console.log(\"ID가 일치합니다.\")\n          }\n          else {\n            console.log(\"ID가 틀렸습니다.\")\n          }\n          //store.commit('updateloginUserID',this.memID)\n          console.log(`login : ${store.state.loginUser.memID}`)\n        }).catch(function (error) {\n          console.log(error)\n        })\n        console.log(\"end\")\n    },\n    onClickRedirect (){\n       window.open(\"http://localhost:8080/\");  \n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}