{"ast":null,"code":"import axios from 'axios';\nimport store from '@/store';\naxios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\nexport default {\n  data() {\n    return {\n      memID: '',\n      memPW: ''\n    };\n  },\n\n  computed: {\n    memID: {\n      get() {\n        return store.state.loginUser.memID;\n      },\n\n      set(value) {\n        this.store.commit(updateloginUser, value);\n      }\n\n    } // loginMemID () {\n    //   let loginUserID = store.state.loginUser.memID\n    //   loginUserID = this.memID\n    //   return loginUserID\n    // }\n\n  },\n  methods: {\n    sendPost() {\n      console.log(this.memID, this.loginMemID);\n      axios.post('http://192.168.0.81:9292/loginProc', {\n        memID: this.loginMemID,\n        memPW: this.memPW\n      }).then(function (res) {\n        console.log(this.memID);\n\n        if (res.data.memID == memID) {\n          console.log(\"ID가 일치합니다.\");\n        } else {\n          console.log(\"ID가 틀렸습니다.\");\n        }\n\n        console.log(res.data);\n        console.log(res.data.mem.memID);\n        console.log(res.data.mem.memRole.roleCode);\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    onClickRedirect() {\n      window.open(\"http://localhost:8080/\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAeA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAEAD,KAAK,CAACE,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,KAAK,EAAE;IAFF,CAAP;EAID,CANY;;EAObC,QAAQ,EAAE;IACRF,KAAK,EAAE;MACLG,GAAE,GAAK;QACL,OAAOR,KAAK,CAACS,KAAN,CAAYC,SAAZ,CAAsBL,KAA7B;MACD,CAHI;;MAILM,GAAE,CAAGC,KAAH,EAAU;QACT,KAAKZ,KAAL,CAAWa,MAAX,CAAkBC,eAAlB,EAAkCF,KAAlC;MACH;;IANK,CADC,CASR;IACA;IACA;IACA;IACA;;EAbQ,CAPG;EAsBbG,OAAO,EAAE;IACPC,QAAO,GAAK;MACVC,OAAO,CAACC,GAAR,CAAY,KAAKb,KAAjB,EAAwB,KAAKc,UAA7B;MACApB,KAAK,CAACqB,IAAN,CAAW,oCAAX,EAAiD;QAC/Cf,KAAK,EAAE,KAAKc,UADmC;QAE/Cb,KAAK,EAAE,KAAKA;MAFmC,CAAjD,