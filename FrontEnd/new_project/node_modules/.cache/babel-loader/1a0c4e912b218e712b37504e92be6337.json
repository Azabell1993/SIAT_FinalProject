{"ast":null,"code":"import axios from 'axios';\nimport store from '@/store';\naxios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\nexport default {\n  computed: {\n    memID: {\n      get() {\n        return store.state.loginUser.memID;\n      },\n\n      set(value) {\n        console.log(value);\n        store.commit('updateloginUserID', value);\n      }\n\n    }\n  },\n  methods: {\n    sendPost() {\n      console.log(this.memID);\n      axios.post('http://192.168.0.107:9292/loginProc', {\n        memID: this.memID,\n        memPW: this.memPW\n      }).then(function (res) {\n        console.log(res.data);\n        console.log(this.memID);\n\n        if (res.data.memID == this.memID) {\n          console.log(\"ID가 일치합니다.\");\n        } else {\n          console.log(\"ID가 틀렸습니다.\");\n        }\n\n        console.log(res.data.mem.memID);\n        console.log(res.data.mem.memRole.roleCode);\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    onClickRedirect() {\n      window.open(\"http://localhost:8080/\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAEAD,KAAK,CAACE,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,eAAe;EACbC,QAAQ,EAAE;IACRC,KAAK,EAAE;MACLC,GAAE,GAAK;QACL,OAAON,KAAK,CAACO,KAAN,CAAYC,SAAZ,CAAsBH,KAA7B;MACD,CAHI;;MAILI,GAAE,CAAGC,KAAH,EAAU;QACVC,OAAO,CAACC,GAAR,CAAYF,KAAZ;QACAV,KAAK,CAACa,MAAN,CAAa,mBAAb,EAAiCH,KAAjC;MACF;;IAPK;EADC,CADG;EAYbI,OAAO,EAAE;IACPC,QAAO,GAAK;MACVJ,OAAO,CAACC,GAAR,CAAY,KAAKP,KAAjB;MACAN,KAAK,CAACiB,IAAN,CAAW,qCAAX,EAAkD;QAChDX,KAAK,EAAE,KAAKA,KADoC;QAEhDY,KAAK,EAAE,KAAKA;MAFoC,CAAlD,EAIGC,IAJH,CAIQ,UAAUC,GAAV,EAAe;QACnBR,OAAO,CAACC,GAAR,CAAYO,GAAG,CAACC,IAAhB;QACAT,OAAO,CAACC,GAAR,CAAY,KAAKP,KAAjB;;QACA,IAAGc,GAAG,CAACC,IAAJ,CAASf,KAAT,IAAkB,KAAKA,KAA1B,EAAgC;UAC9BM,OAAO,CAACC,GAAR,CAAY,YAAZ;QACF,CAFA,MAGK;UACHD,OAAO,CAACC,GAAR,CAAY,YAAZ;QACF;;QACAD,OAAO,CAACC,GAAR,CAAYO,GAAG,CAACC,IAAJ,CAASC,GAAT,CAAa