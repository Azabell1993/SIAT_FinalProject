{"ast":null,"code":"axios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\nimport AdminCategory from '@/components/AdminCategory.vue';\nexport default {\n  components: {\n    AdminCategory\n  },\n\n  data() {\n    //변수 생성\n    return {\n      product: {\n        proName: null,\n        // 상품명 \n        proPrice: null,\n        // 가격\n        proStock: '20',\n        // 재고\n        proDetail: '이곳에 상세 설명을 입력해주세요.',\n        // 상세설명\n        categorySmallName: null,\n        // 카테고리 소분류 이름\n        genderName: null,\n        //성별 속성 이름\n        colorName: null,\n        //색깔 속성 이름\n        materialName: null,\n        // 재질 속성 이름\n        ageName: null,\n        // 나이 속성 이름\n        priceRangeName: null // 가격대 속성 이름\n\n      }\n    };\n  },\n\n  methods: {\n    fnList() {\n      //리스트 화면으로 이동 함수\n      this.$router.push({\n        path: './list',\n        query: this.body\n      });\n    },\n\n    fnAddProc() {\n      //등록 프로세스\n      if (!this.subject) {\n        //제목이 없다면 값을 입력하라고 알려준다.\n        alert(\"제목을 입력해 주세요\");\n        this.$refs.subject.focus(); //방식으로 선택자를 찾는다.\n\n        return;\n      }\n\n      this.form = {\n        //backend로 전송될 POST 데이터\n        board_code: this.board_code,\n        subject: this.subject,\n        cont: this.cont,\n        id: this.id\n      };\n      this.$axios.post('http://localhost:3000/api/board', this.form).then(res => {\n        if (res.data.success) {\n          alert('등록되었습니다.');\n          this.fnList();\n        } else {\n          alert(\"실행중 실패했습니다.\\n다시 이용해 주세요\");\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4CAA,KAAK,CAACC,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,OAAOC,aAAP,MAA0B,gCAA1B;AACA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;;EAKdE,IAAI,GAAG;IAAE;IACR,OAAM;MACFC,OAAO,EAAE;QACPC,OAAO,EAAC,IADD;QACa;QACpBC,QAAQ,EAAC,IAFF;QAEa;QACpBC,QAAQ,EAAC,IAHF;QAGa;QACpBC,SAAS,EAAC,oBAJH;QAI6B;QACpCC,iBAAiB,EAAC,IALX;