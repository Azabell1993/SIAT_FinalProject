{"ast":null,"code":"import axios from 'axios';\nimport storeProduct from '@/store/recommendProducts';\nconst productCodeList = Object.values(storeProduct.state.originProductList);\nexport default {\n  data() {\n    return {\n      categoryIndex: []\n    };\n  },\n\n  mounted() {\n    var vm = this;\n    axios.post('http://192.168.0.81:9292/pro/categoryData').then(function (response) {\n      vm.categoryIndex = response.data; // console.log('상품 데이터 : ',response.data)\n    }).catch(function (error) {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    topdata() {\n      var topCodeList = [];\n      axios.post('http://192.168.0.81:9292/pro/proListByCategory', {\n        proList: productCodeList,\n        categoryLargeName: '상의',\n        categorySmallName: 'none'\n      }).then(function (response) {\n        // console.log(response.data)\n        topCodeList.push(response.data.data);\n        storeProduct.state.categoryTopList = topCodeList; // console.log('new Top code list',topCodeList)\n        // vm.$router.push({\n        // name: \"상의\",\n        // query: { data : topCodeList }\n        // })\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    pantdata() {\n      var pantCodeList = [];\n      axios.post('http://192.168.0.81:9292/pro/proListByCategory', {\n        proList: productCodeList,\n        categoryLargeName: '바지',\n        categorySmallName: 'none'\n      }).then(function (response) {\n        pantCodeList.push(response.data.data);\n        storeProduct.commit('updateCategoryPantsList', pantCodeList); // storeProduct.state.categoryPantList = pantCodeList\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    shoesdata() {\n      var shoesCodeList = [];\n      axios.post('http://192.168.0.81:9292/pro/proListByCategory', {\n        proList: productCodeList,\n        categoryLargeName: '신발',\n        categorySmallName: 'none'\n      }).then(function (response) {\n        shoesCodeList.push(response.data.data);\n        storeProduct.commit('updateCategoryShoesList', shoesCodeList); // storeProduct.state.categoryShoesList = shoesCodeList\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    skirtdata() {\n      var skirtCodeList = [];\n      axios.post('http://192.168.0.81:9292/pro/proListByCategory', {\n        proList: productCodeList,\n        categoryLargeName: '치마',\n        categorySmallName: 'none'\n      }).then(function (response) {\n        skirtCodeList.push(response.data.data);\n        console.log('skirtCodeList : ', skirtCodeList);\n        storeProduct.commit('updateCategorySkirtList', skirtCodeList); // storeProduct.state.categorySkirtList = skirtCodeList\n      }).catch(function (error) {\n        console.log(error);\n      });\n    },\n\n    hatdata() {\n      var hatCodeList = [];\n      axios.post('http://192.168.0.81:9292/pro/proListByCategory', {\n        proList: productCodeList,\n        categoryLargeName: '모자',\n        categorySmallName: 'none'\n      }).then(function (response) {\n        hatCodeList.push(response.data.data); // storeProduct.state.categoryHatList = hatCodeList\n\n        storeProduct.commit('updateCategoryHatList', hatCodeList);\n      }).catch(function (error) {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,YAAP,MAAyB,2BAAzB;AAEA,MAAMC,eAAc,GAAIC,MAAM,CAACC,MAAP,CAAcH,YAAY,CAACI,KAAb,CAAmBC,iBAAjC,CAAxB;AAEA,eAAe;EACbC,IAAG,GAAK;IACN,OAAO;MACLC,aAAY,EAAI;IADX,CAAP;EAGD,CALY;;EAObC,OAAM,GAAK;IACT,IAAIC,EAAC,GAAI,IAAT;IACAV,KAAK,CAACW,IAAN,CAAW,2CAAX,EACKC,IADL,CACU,UAAUC,QAAV,EAAoB;MACxBH,EAAE,CAACF,aAAH,GAAmBK,QAAQ,CAACN,IAA5B,CADwB,CAExB;IACD,CAJL,EAKKO,KALL,CAKW,UAAUC,KAAV,EAAiB;MACtBC,OAAO,CAACC,GAAR,CAAYF,KAAZ;IACD,CAPL;EAQD,CAjBY;;EAkBZG,OAAO,EAAE;IACRC,OAAM,GAAK;MACT,IAAIC,WAAU,GAAI,EAAlB;MACApB,KAAK,CAACW,IAAN,CAAW,gDAAX,EACA;QACEU,OAAO,EAAEnB,eADX;QAEEoB,iBAAgB,EAAI,I