ageNameOption Name Value */\n\n            var select = document.getElementById('ageNameOption');\n            var ageNameOption_value = select.options[select.selectedIndex];\n            console.log(ageNameOption_value);\n            /* 6 */\n\n            /* Test Get priceRangeNameOption Name Value */\n\n            var select = document.getElementById('priceRangeNameOption');\n            var priceRangeNameOption_value = select.options[select.selectedIndex];\n            console.log(priceRangeNameOption_value);\n            /* 7 */\n\n            /* Test Get priceRangeNameOption Name Value */\n\n            var select = document.getElementById('seasonNameOption');\n            var seasonNameOption_value = select.options[select.selectedIndex];\n            console.log(seasonNameOption_value);\n          } else {\n            alert(\"false\");\n          }\n        }).catch(function (error) {\n          console.log(error);\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+JA,OAAOA,KAAP,MAAkB,OAAlB;AAEAA,KAAK,CAACC,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,OAAOC,aAAP,MAA0B,gCAA1B;AACA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;;EAKbE,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,OAAO,EAAE,QADF;QACkB;QACzBC,QAAQ,EAAC,OAFF;QAEgB;QACvBC,QAAQ,EAAE,IAHH;QAGc;QACrBC,SAAS,EAAE,WAJJ;QAIqB;;QAE5B;QACAC,iBAAiB,EAAC,IAPX;QAQPC,iBAAiB,EAAC,IARX;QAUPC,YAAY,EAAE,IAVP;QAUa;QACpBC,UAAU,EAAE,IAXL;QAWc;QACrBC,SAAS,EAAE,IAZJ;QAYc;QACrBC,YAAY,EAAE,IAbP;QAac;QACrBC,OAAO,EAAE,IAdF;QAcc;QACrBC,cAAc,EAAE,IAfT;QAee;QACtBC,UAAU,EAAC,IAhBJ,CAgBe;;MAhBf,CADJ;MAmBLC,sBAAsB,EAAC,EAnBlB;MAoBLC,sBAAsB,EAAC,EApBlB;MAqBLC,WAAW,EAAE,EArBR;MAsBLC,UAAU,EAAE,EAtBP;MAuBLC,aAAa,EAAE,EAvBV;MAwBLC,QAAQ,EAAC,EAxBJ;MAyBLC,UAAS,EAAI,EAzBR;MA0BLC,WAAW,EAAE;IA1BR,CAAP;EA4BD,CAlCY;;EAoCbC,OAAM,GAAK;IACT,IAAIC,EAAC,GAAI,IAAT;IAGA;;IACA,IAAGA,EAAE,CAACT,sBAAN,EAA8B;MAC1BrB,KAAK,CAAC+B,IAAN,CAAW,2CAAX,EACCC,IADD,CACM,UAAUC,QAAV,EAAoB;QACxBH,EAAE,CAACT,sBAAH,GAA4BY,QAAQ,CAAC3B,IAArC;QA