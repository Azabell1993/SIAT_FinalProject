ios.post('http://localhost:9292/pro/proInfo', \n        {proCode : shoesList[i]})\n        .then(function (response) {\n          console.log(response.data.data);\n          vm.productShoesInfos.push(response.data.data)\n        })\n        .catch(function (error) {\n          console.log(error);\n        })\n      }\n      }\n    }\n}\n</script>\n\n<style>\n\n</style>\n"]},"metadata":{},"sourceType":"module"}