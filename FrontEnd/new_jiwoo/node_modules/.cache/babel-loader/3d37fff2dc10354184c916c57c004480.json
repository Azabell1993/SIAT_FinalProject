{"ast":null,"code":"import axios from 'axios';\nimport store from '@/store/index';\naxios.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\nexport default {\n  methods: {\n    sendPost() {\n      const userId = this.memID; //변수 선언 이 값을 그대로 쓰겠다. 자스 내에선 this 선언 부분이 다를 수있음. 당분간 이게 좀 안전할 듯\n      //console.log(userId);\n\n      axios.post('http://192.168.0.81:9292/mem/loginProc', {\n        memID: this.memID,\n        memPW: this.memPW\n      }).then(function (response) {\n        //될떄 실행하는 함수\n        //console.log(response.data)\n        if (response.data.mem.memID == userId) {\n          console.log(\"ID가 일치합니다.\"); // location.href = \"http://localhost:8080/\"\n\n          axios.post('http://192.168.0.81:9292/mem/memberInfo', {\n            memID: userId,\n            memPW: userPW,\n            memName: userName,\n            memAddr: userAddr,\n            memPhone: userPhone,\n            memBirth: userBirth,\n            memRole: userRole\n          }).then(function (response) {\n            // console.log(response.data)\n            // console.log('memPW : ',response.data.data.memPW)\n            store.commit('updateloginUserID', userId); //로그인이 성공하는 과정에서 memID를 조회해서 정보를 가져오는건가?\n\n            store.commit('updateloginUserPW', response.data.data.memPW);\n            console.log('memPW : ', response.data.data.memPW);\n            store.commit('updateloginUserPW', response.data.data.memName);\n            console.log('memName : ', response.data.data.memName);\n            console.log('store에 있는 NAME : ', store.state.loginUser.memName);\n            store.commit('updateloginUserPW', response.data.data.memAddr);\n            console.log('memAddr : ', response.data.data.memAddr);\n            store.commit('updateloginUserPW', response.data.data.memPhone);\n            console.log('memPhone : ', response.data.data.memPhone);\n            store.commit('updateloginUserPW', response.data.data.memBirth);\n            console.log('memBirth : ', response.data.data.memBirth);\n            store.commit('updateloginUserPW', response.data.data.memRole.roleCode);\n            console.log('memRole : ', response.data.data.memRole.roleCode);\n          });\n        } else {\n          alert('ID 혹은 비밀번호가 틀렸습니다');\n        } //store.commit('updateloginUserID',this.memID)\n\n\n        console.log(`login : ${store.state.loginUser.memID}`); //store에 있는 ID 값 체크\n      }).catch(function (error) {\n        // 뭔가 실패할때 쓰는 함수\n        console.log(error);\n      });\n      console.log(\"end\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,eAAlB;AAEAD,KAAK,CAACE,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,eAAe;EACbC,OAAO,EAAE;IACPC,QAAO,GAAK;MACV,MAAMC,MAAK,GAAI,KAAKC,KAApB,CADU,CACiB;MAC3B;;MAEAR,KAAK,CAACS,IAAN,CAAW,wCAAX,EAAqD;QACnDD,KAAK,EAAE,KAAKA,KADuC;QAEnDE,KAAK,EAAE,KAAKA;MAFuC,CAArD,EAIGC,IAJH,CAIQ,UAAUC,QAAV,EAAoB;QAAE;QAC1B;QACA,IAAGA,QAAQ,CAACC,IAAT,CAAcC,GAAd,CAAkBN,KAAlB,IAA2BD,MAA9B,EAAqC;UACnCQ,OAAO,CAACC,GAAR,CAAY,YAAZ,EADmC,CAEnC;;UAEAhB,KAAK,CAACS,IAAN,CAAW,yCAAX,EACA;YACED,KAAI,EAAID,MADV;YAEEG,KAAI,EAAIO,MAFV;YAGEC,OAAM,EAAIC,QAHZ;YAIEC,OAAM,EAAIC,QAJZ;YAKEC,QAAO,EAAIC,SALb;YAMEC,QAAO,EAAIC,SANb;YAOEC,OAAM,EAAIC;UAPZ,CADA,EAUChB,IAVD,CAUM,UAASC,QAAT,EAAmB;YACvB;YACA;YACAX,KAAK,CAAC2B,MAAN,CAAa,mBAAb,EAAkCrB,MAAlC,EAHuB,CAGmB;;YAE1CN,KAAK,CAAC2B,MAAN,CAAa,mBAAb,EAAkChB,QAAQ,CAACC,IAAT,CAAcA,IAAd,CAAmBH,KAArD;YACAK,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwBJ,QAAQ,CAACC,IAAT,CAAcA,IAAd,CAAmBH,KAA3C;YAEAT,KAAK,CAAC2B,MAAN,CAAa,mBAAb,EAAkChB,QAAQ,CAACC,IAAT,CAAcA,IAAd,CAAmBK,OAArD;YACAH,OAAO,CAACC,GAAR,CAAY,YAAZ,EAA0BJ,QAAQ,CAACC,IAAT,CAAcA,IAAd,CAAmBK,OAA7C;YACAH,OAAO,CAACC,GAAR,CAAY,mBAAZ,EAAgCf,KAAK,CAAC4B,KAAN,CAAYC,SAAZ,CAAsBZ,OAAtD;YAEAjB,KAAK,CAAC2B,MAAN,CAAa,mBAAb,EAAkChB,QAAQ,CAACC,IAAT,CAAcA,IAAd,CAAmBO,OAArD;YACAL,OAAO,CAACC,GAAR,CAAY,YAAZ,EAA0BJ,QAAQ,CAACC,IAAT,CAAcA,IAAd,CAAmBO,OAA7C;YAEAnB,KAAK,CAAC2B,MAAN,CAAa,mBAAb,EAAkChB,QAAQ,CAACC,IAAT,CAAcA,IAAd,C