ame: this.product.materialName,\n          ageName: this.product.ageName,\n          priceRangeName: this.product.priceRangeName\n        }).then(function (datatest) {\n          if (datatest.data.data !== 'true') {\n            alert(\"true\"); // select내 id에서 값을 참조하여 가져온다.\n\n            var select = document.getElementById('categorySmallNameOption');\n            var categorySmallNameOption_value = select.options[select.selectedIndex].value;\n            console.log(categorySmallNameOption_value);\n          } else {\n            alert(\"false\");\n          }\n        }).catch(function (error) {\n          console.log(error);\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyFA,OAAOA,KAAP,MAAkB,OAAlB;AAEAA,KAAK,CAACC,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,6BAA9B,IAA+D,GAA/D;AAEA,OAAOC,aAAP,MAA0B,gCAA1B;AACA,eAAe;EACbC,UAAU,EAAE;IACVD;EADU,CADC;;EAKbE,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,OAAO,EAAC,IADD;QACa;QACpBC,QAAQ,EAAC,IAFF;QAEa;QACpBC,QAAQ,EAAC,IAHF;QAGa;QACpBC,SAAS,EAAC,IAJH;QAIa;QACpBC,iBAAiB,EAAC,IALX;QAKiB;QACxBC,UAAU,EAAC,IANJ;QAMa;QACpBC,SAAS,EAAC,IAPH;QAOa;QACpBC,YAAY,EAAC,IARN;QAQa;QACpBC,OAAO,EAAC,IATD;QASa;QACpBC,cAAc,EAAC,IAVR,CAUa;;MAVb,CADJ;MAaLC,aAAY,EAAI;IAbX,CAAP;EAeD,CArBY;;EAsBb;EACAC,OAAM,GAAK;IACT,IAAIC,EAAC,GAAI,IAAT;IACApB,KAAK,CAACqB,IAAN,CAAW,2CAAX,EACKC,IADL,CACU,UAAUC,QAAV,EAAoB;MACxBH,EAAE,CAACF,aAAH,GAAmBK,QAAQ,CAACjB,IAA5B;MACAkB,OAAO,CAACC,GAAR,CAAY,WAAZ,EAAwBF,QAAQ,CAACjB,IAAjC;IACD,CAJL,EAKKoB,KALL,CAKW,UAAUC,KAAV,EAAiB;MACtBH,OAAO,CAACC,GAAR,CAAYE,KAAZ;IACD,CAPL;EAQC,CAjCU;;EAmCbC,OAAO,EAAE;IACP;IACAC,eAAe,GAAG;MAEhBL,OAAO,CAACC,GAAR,CAAY,cAAZ,EAFgB,CAGhB;MACA;MACA;;MAEA,IAAIK,EAAC,GAAI,IAAT;;MACA,IAAG,CAACA,EAAE,CAACvB,OAAH,CAAWK,iBAAf,EAAkC;QAChCmB,KAAK,CAAC,sBAAD,CAAL;QACA,KAAKC,KAAL,CAAWC,sBAAX,CAAkCC,KAAlC,GAFgC,CAEW;;QAC3C;MACF,CAJA,MAIO;QACLlC,KAAK,CAACqB,IAAN,CAAW,qCAAX,EAAkD;UAChDb,OAAO,EAAE,KAAKD,OAAL,CAAaC,OAD0B;UAEhDC,QAAQ,EAAE,KAAKF,OAAL,CAAaE,QAFyB;UAGhDC,QAAQ,EAAE,KAAKH,OAAL,CAAaG,QAH