<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.siat.blueclub.persistence.ProductDao">

    <select id="getProductsByCategoryCode" parameterType="Integer" resultType="com.siat.blueclub.domain.ProductVO">
    	select * from Product where category_code=#{categoryCode}
    </select>
    <select id="getProductsByCategoryCodeRange" parameterType="Integer" resultType="com.siat.blueclub.domain.ProductVO">
    	select * from Product where category_code between #{categoryRange1} and #{categoryRange2}
    </select>
    
    
</mapper>
